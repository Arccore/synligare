<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.graphiti.ui.diagramTypeProviders">
      <diagramTypeProvider
            class="org.eclipse.eatop.examples.graphicaleditor.dtpd.providers.DTPDTypeDiagramProvider"
            id="org.eclipse.eatop.examples.graphicaleditor.dtpd.TypeDiagramProvider"
            name="org.eclipse.eatop.examples.graphicaleditor.dtpd">
         <diagramType
               id="org.eclipse.eatop.examples.graphicaleditor.dtpd.DiagramType">
         </diagramType>
         <imageProvider
               id="org.eclipse.eatop.examples.graphicaleditor.dtpd.imageProvider">
         </imageProvider>
      </diagramTypeProvider>
   </extension>
   <extension
         point="org.eclipse.graphiti.ui.diagramTypes">
      <diagramType
            description="This is a Graphiti Example"
            id="org.eclipse.eatop.examples.graphicaleditor.dtpd.DiagramType"
            name="org.eclipse.eatop.examples.graphicaleditor.dtpd"
            type="org.eclipse.eatop.examples.graphicaleditor.dtpd">
      </diagramType>
   </extension>
   <extension
         point="org.eclipse.graphiti.ui.imageProviders">
      <imageProvider
            class="org.eclipse.eatop.examples.graphicaleditor.dtpd.providers.DTPDDImageProvider"
            id="org.eclipse.eatop.examples.graphicaleditor.dtpd.imageProvider">
      </imageProvider>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="org.eclipse.eatop.examples.graphicaleditor.dtpd.PropertyContributor">
         <propertyTab
               category="Graphiti"
               id="graphiti.advanced.tab"
               label="Advanced">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="org.eclipse.eatop.examples.graphicaleditor.dtpd.PropertyContributor">
         <propertySection
               class="org.eclipse.eatop.examples.graphicaleditor.dtpd.property.DTPDPropertySection"
               filter="org.eclipse.eatop.examples.graphicaleditor.dtpd.property.DTPDPropertyFilter"
               id="graphiti.advanced.tab.dtpd"
               tab="graphiti.advanced.tab">
         </propertySection>
      </propertySections>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="org.eclipse.eatop.examples.graphicaleditor.dtpd.PropertyContributor">
         <propertyCategory
               category="Graphiti"></propertyCategory>
      </propertyContributor>
   </extension>

</plugin>
