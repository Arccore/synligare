<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.graphiti.ui.diagramTypeProviders">
      <diagramTypeProvider
            class="org.eclipse.eatop.examples.graphicaleditor.hctd.providers.HCTDTypeDiagramProvider"
            id="org.eclipse.eatop.examples.graphicaleditor.hctd.TypeDiagramProvider"
            name="org.eclipse.eatop.examples.graphicaleditor.hctd">
         <diagramType
               id="org.eclipse.eatop.examples.graphicaleditor.hctd.DiagramType">
         </diagramType>
         <imageProvider
               id="org.eclipse.eatop.examples.graphicaleditor.hctd.imageProvider">
         </imageProvider>
      </diagramTypeProvider>
   </extension>
   <extension
         point="org.eclipse.graphiti.ui.diagramTypes">
      <diagramType
            description="This is a Graphiti Example"
            id="org.eclipse.eatop.examples.graphicaleditor.hctd.DiagramType"
            name="org.eclipse.eatop.examples.graphicaleditor.hctd"
            type="org.eclipse.eatop.examples.graphicaleditor.hctd">
      </diagramType>
   </extension>
   <extension
         point="org.eclipse.graphiti.ui.imageProviders">
      <imageProvider
            class="org.eclipse.eatop.examples.graphicaleditor.hctd.providers.HCTDImageProvider"
            id="org.eclipse.eatop.examples.graphicaleditor.hctd.imageProvider">
      </imageProvider>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="org.eclipse.eatop.examples.graphicaleditor.hctd.PropertyContributor">
         <propertyCategory
               category="Graphiti"></propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="org.eclipse.eatop.examples.graphicaleditor.hctd.PropertyContributor">
         <propertyTab
               category="Graphiti"
               id="graphiti.advanced.tab"
               label="Advanced">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="org.eclipse.eatop.examples.graphicaleditor.hctd.PropertyContributor">
         <propertySection
               class="org.eclipse.eatop.examples.graphicaleditor.hctd.property.HCTDPropertySection"
               filter="org.eclipse.eatop.examples.graphicaleditor.hctd.property.HCTDPropertyFilter"
               id="graphiti.advanced.tab.hctd"
               tab="graphiti.advanced.tab">
         </propertySection>
         <propertySection
               afterSection="graphiti.advanced.tab.hctd"
               class="org.eclipse.eatop.examples.graphicaleditor.hctd.property.HCTDPropertySectionProtoTypePartOF"
               filter="org.eclipse.eatop.examples.graphicaleditor.hctd.property.HCTDPropertyprototypeFilter"
               id="graphiti.advanced.tab.aftd2"
               tab="graphiti.advanced.tab">
         </propertySection>
      </propertySections>
   </extension>

</plugin>
