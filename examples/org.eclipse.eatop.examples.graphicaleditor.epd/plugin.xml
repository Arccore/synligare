<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.graphiti.ui.diagramTypes">
      <diagramType
            id="org.eclipse.eatop.examples.graphicaleditor.epd.diagramType"
            name="org.eclipse.eatop.examples.graphicaleditor.epd"
            type="org.eclipse.eatop.examples.graphicaleditor.epd">
      </diagramType>
   </extension>
   <extension
         point="org.eclipse.graphiti.ui.diagramTypeProviders">
      <diagramTypeProvider
            class="org.eclipse.eatop.examples.graphicaleditor.epd.provider.EPDDiagramTypeProvider"
            description="EAPackage Diagram"
            id="org.eclipse.eatop.examples.graphicaleditor.epd.diagramTypeProvider"
            name="org.eclipse.eatop.examples.graphicaleditor.epd">
         <diagramType
               id="org.eclipse.eatop.examples.graphicaleditor.epd.diagramType">
         </diagramType>
         <imageProvider
               id="org.eclipse.eatop.examples.graphicaleditor.epd.imageProvider">
         </imageProvider>
      </diagramTypeProvider>
   </extension>
   <extension
         point="org.eclipse.graphiti.ui.imageProviders">
      <imageProvider
            class="org.eclipse.eatop.examples.graphicaleditor.epd.provider.EPDImageProvider"
            id="org.eclipse.eatop.examples.graphicaleditor.epd.imageProvider">
      </imageProvider>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="org.eclipse.eatop.examples.graphicaleditor.epd.PropertyContributor">
         <propertyCategory
               category="Graphiti"></propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="org.eclipse.eatop.examples.graphicaleditor.epd.PropertyContributor">
         <propertyTab
               category="Graphiti"
               id="graphiti.advanced.tab"
               label="Advanced">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="org.eclipse.eatop.examples.graphicaleditor.epd.PropertyContributor">
         <propertySection
               class="org.eclipse.eatop.examples.graphicaleditor.epd.property.EPDPropertySection"
               filter="org.eclipse.eatop.examples.graphicaleditor.epd.property.EPDPropertyFilter"
               id="graphiti.advanced.tab.epd"
               tab="graphiti.advanced.tab">
         </propertySection>
      </propertySections>
   </extension>
   

</plugin>
