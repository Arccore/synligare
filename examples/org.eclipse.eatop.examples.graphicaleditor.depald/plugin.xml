<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.graphiti.ui.diagramTypeProviders">
      <diagramTypeProvider
            class="org.eclipse.eatop.examples.graphicaleditor.depald.provider.DEPALDDiagramTypeProvider"
            description="DEPAL Diagram"
            id="org.eclipse.eatop.examples.graphicaleditor.depald.diagramTypeProvider"
            name="org.eclipse.eatop.examples.graphicaleditor.depald">
         <diagramType
               id="org.eclipse.eatop.examples.graphicaleditor.depald.diagramType">
         </diagramType>
         <imageProvider
               id="org.eclipse.eatop.examples.graphicaleditor.depald.imageProvider">
         </imageProvider>
      </diagramTypeProvider>
   </extension>
   <extension
         point="org.eclipse.graphiti.ui.diagramTypes">
      <diagramType
            id="org.eclipse.eatop.examples.graphicaleditor.depald.diagramType"
            name="org.eclipse.eatop.examples.graphicaleditor.depald"
            type="org.eclipse.eatop.examples.graphicaleditor.depald">
      </diagramType>
   </extension>
   <extension
         point="org.eclipse.graphiti.ui.imageProviders">
      <imageProvider
            class="org.eclipse.eatop.examples.graphicaleditor.depald.provider.DEPALDImageProvider"
            id="org.eclipse.eatop.examples.graphicaleditor.depald.imageProvider">
      </imageProvider>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="org.eclipse.eatop.examples.graphicaleditor.depald.PropertyContributor">
         <propertyCategory
               category="Graphiti">
         </propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="org.eclipse.eatop.examples.graphicaleditor.depald.PropertyContributor">
         <propertyTab
               category="Graphiti"
               id="graphiti.advanced.tab"
               label="Advanced">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="org.eclipse.eatop.examples.graphicaleditor.depald.PropertyContributor">
         <propertySection
               class="org.eclipse.eatop.examples.graphicaleditor.depald.property.DEPALDAPropertySection"
               filter="org.eclipse.eatop.examples.graphicaleditor.depald.property.DEPALDAPropertyFilter"
               id="graphiti.advanced.tab.depald"
               tab="graphiti.advanced.tab">
         </propertySection>
      </propertySections>
   </extension>

</plugin>
