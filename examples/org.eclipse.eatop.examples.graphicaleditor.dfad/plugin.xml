<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.graphiti.ui.diagramTypeProviders">
      <diagramTypeProvider
            class="org.eclipse.eatop.examples.graphicaleditor.dfad.providers.DFADTypeDiagramProvider"
            id="org.eclipse.eatop.examples.graphicaleditor.dfad.TypeDiagramProvider"
            name="org.eclipse.eatop.examples.graphicaleditor.dfad">
         <diagramType
               id="org.eclipse.eatop.examples.graphicaleditor.dfad.DiagramType">
         </diagramType>
         <imageProvider
               id="org.eclipse.eatop.examples.graphicaleditor.dfad.imageProvider">
         </imageProvider>
      </diagramTypeProvider>
   </extension>
   <extension
         point="org.eclipse.graphiti.ui.diagramTypes">
      <diagramType
            description="This is a Graphiti Example"
            id="org.eclipse.eatop.examples.graphicaleditor.dfad.DiagramType"
            name="org.eclipse.eatop.examples.graphicaleditor.dfad"
            type="org.eclipse.eatop.examples.graphicaleditor.dfad">
      </diagramType>
   </extension>
   <extension
         point="org.eclipse.graphiti.ui.imageProviders">
      <imageProvider
            class="org.eclipse.eatop.examples.graphicaleditor.dfad.providers.DFADImageProvider"
            id="org.eclipse.eatop.examples.graphicaleditor.dfad.imageProvider">
      </imageProvider>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="org.eclipse.eatop.examples.graphicaleditor.dfad.PropertyContributor">
         <propertyTab
               category="Graphiti"
               id="graphiti.advanced.tab"
               label="Advanced">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="org.eclipse.eatop.examples.graphicaleditor.dfad.PropertyContributor">
         <propertySection
               class="org.eclipse.eatop.examples.graphicaleditor.dfad.property.DFADPropertySection"
               filter="org.eclipse.eatop.examples.graphicaleditor.dfad.property.DFADPropertyFilter"
               id="graphiti.advanced.tab.dfad"
               tab="graphiti.advanced.tab">
         </propertySection>
      </propertySections>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="org.eclipse.eatop.examples.graphicaleditor.dfad.PropertyContributor">
         <propertyCategory
               category="Graphiti"></propertyCategory>
      </propertyContributor>
   </extension>

</plugin>
