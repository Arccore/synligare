<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.graphiti.ui.diagramTypeProviders">
      <diagramTypeProvider
            class="org.eclipse.eatop.examples.graphicaleditor.aftd.providers.AFTDTypeDiagramProvider"
            id="org.eclipse.eatop.examples.graphicaleditor.aftd.TypeDiagramProvider"
            name="org.eclipse.eatop.examples.graphicaleditor.aftd">
         <diagramType
               id="org.eclipse.eatop.examples.graphicaleditor.aftd.DiagramType">
         </diagramType>
         <imageProvider
               id="org.eclipse.eatop.examples.graphicaleditor.aftd.imageProvider">
         </imageProvider>
      </diagramTypeProvider>
   </extension>
   <extension
         point="org.eclipse.graphiti.ui.diagramTypes">
      <diagramType
            description="This is a Graphiti Example"
            id="org.eclipse.eatop.examples.graphicaleditor.aftd.DiagramType"
            name="org.eclipse.eatop.examples.graphicaleditor.aftd"
            type="org.eclipse.eatop.examples.graphicaleditor.aftd">
      </diagramType>
   </extension>
   <extension
         point="org.eclipse.graphiti.ui.imageProviders">
      <imageProvider
            class="org.eclipse.eatop.examples.graphicaleditor.aftd.providers.AFTDImageProvider"
            id="org.eclipse.eatop.examples.graphicaleditor.aftd.imageProvider">
      </imageProvider>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="org.eclipse.eatop.examples.graphicaleditor.aftd.PropertyContributor">
         <propertyTab
               category="Graphiti"
               id="graphiti.advanced.tab"
               label="Advanced">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="org.eclipse.eatop.examples.graphicaleditor.aftd.PropertyContributor">
         <propertyCategory
               category="Graphiti"></propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="org.eclipse.eatop.examples.graphicaleditor.aftd.PropertyContributor">
         <propertySection
               class="org.eclipse.eatop.examples.graphicaleditor.aftd.property.AFTDPropertySection"
               filter="org.eclipse.eatop.examples.graphicaleditor.aftd.property.AFTDPropertyFilter"
               id="graphiti.advanced.tab.aftd"
               tab="graphiti.advanced.tab">
         </propertySection>
         <propertySection
               afterSection="graphiti.advanced.tab.aftd"
               class="org.eclipse.eatop.examples.graphicaleditor.aftd.property.AFTDPropertySectionProtoTypePartOF"
               filter="org.eclipse.eatop.examples.graphicaleditor.aftd.property.AFTDPropertyprototypeFilter"
               id="graphiti.advanced.tab.aftd2"
               tab="graphiti.advanced.tab">
         </propertySection>
      </propertySections>
   </extension>

</plugin>
